                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/category_button"
                app:layout_constraintWidth_percent=".3922"
                app:rippleColor="@color/transparent" />

            <RadioButton
                android:id="@+id/category_button4"
