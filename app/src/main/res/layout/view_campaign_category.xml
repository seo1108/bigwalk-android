                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginStart="2dp"
                android:background="@drawable/selector_radio_category4"
                android:button="@null"
                android:minWidth="57dp"
                android:minHeight="28dp"
                android:tag="10"
                android:textAlignment="center"
                android:textColor="@color/white"
                app:layout_constraintBottom_toBottomOf="@+id/category_button3"
                app:layout_constraintDimensionRatio="1:0.88"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/category_button3"
                app:layout_constraintTop_toTopOf="@+id/category_button3"
                app:layout_constraintWidth_percent=".3922"
                app:rippleColor="@color/transparent" />

            <RadioButton
                android:id="@+id/category_button5"
                style="@style/NanumSquareR14White"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginTop="1dp"
                android:background="@drawable/selector_radio_category5"
                android:button="@null"
                android:minWidth="57dp"
                android:minHeight="28dp"
                android:tag="8"
                android:textAlignment="center"
                android:textColor="@color/white"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintDimensionRatio="1:0.88"
                app:layout_constraintEnd_toStartOf="@+id/category_button6"
                app:layout_constraintHorizontal_chainStyle="packed"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/category_button3"
                app:layout_constraintWidth_percent=".3922"
                app:rippleColor="@color/transparent" />

            <RadioButton
                android:id="@+id/category_button6"
                style="@style/NanumSquareR14White"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginStart="2dp"
                android:background="@drawable/selector_radio_category6"
                android:button="@null"
                android:minWidth="57dp"
                android:minHeight="28dp"
                android:tag="7"
                android:textAlignment="center"
                android:textColor="@color/white"
                app:layout_constraintBottom_toBottomOf="@+id/category_button5"
                app:layout_constraintDimensionRatio="1:0.88"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/category_button5"
                app:layout_constraintTop_toTopOf="@+id/category_button5"
                app:layout_constraintWidth_percent=".3922"
                app:rippleColor="@color/transparent" />
        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>